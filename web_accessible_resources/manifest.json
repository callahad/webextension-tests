{
	"manifest_version": 2,
	"name": "WebExtension Test - web_accessible_resources",
	"version": "0.0.1",
	"applications": {
		"gecko": {
			"id": "test-webext-war@example.com"
		}
	},
	"content_scripts": [
		{
			"matches": [
				"*://*.example.com/*"
			],
			"js": [
				"content_script.js"
			]
		}
	],
	"web_accessible_resources": [
		"resource.json"
	]
}
